digraph "automata drawer" {
	rankdir=LR
	splines=spline
	node [shape=circle]
	edge [arrowsize=0.8]
	nullplace [label="" fixedsize=shape shape=plaintext width=0.01]
	nullplace -> 0
	1 [label=1 shape=doublecircle]
	0 -> 1 [label=a]
	0 -> 2 [label=b]
	1 -> 1 [label="a, b"]
	2 -> 1 [label=a]
	2 -> 2 [label=b]
}
