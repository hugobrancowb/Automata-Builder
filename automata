digraph "automata drawer" {
	rankdir=LR
	node [shape=circle]
	edge [arrowsize=0.8]
	nullplace [label="" shape=plaintext]
	nullplace -> 3 [shape=invis]
	1 [label=1 shape=doublecircle]
	3 [label=3 shape=doublecircle]
	0 -> 0 [label=a]
	0 -> 1 [label=b]
	0 -> 2 [label=E]
	1 -> 1 [label=a]
	1 -> 1 [label=b]
	1 -> 2 [label=E]
	2 -> 1 [label=a]
	2 -> 2 [label=b]
	2 -> 3 [label=c]
	3 -> 0 [label=a]
	3 -> 3 [label=c]
}
